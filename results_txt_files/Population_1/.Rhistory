myPairs <- ggpairs(p_values_final[,1:10],
diag = list(continuous = "density",weights=p_values_final[,11]),
upper = "blank",
lower = list(continuous = "density", combo = "box",weights=p_values_final[,11]),
axisLabels = "show",
title="")
print(myPairs)
